{
	"method" : "post",
	"id" : "dbUpload",
	"action" : "/import/submit",
	"class" : "form-horizontal",
	"enctype" : "multipart/form-data",
	"elements" : 
	[
		{	"type" : "text",
			"name" : "separator",
			"value" : ";",
			"validators" : 
			[ 
				"required"
			]
		},
		{	"type" : "text",
			"name" : "enclosure",
			"value" : "\"",
			"validators" : 
			[ 
				"required"
			]
		},
		{	"type" : "select",
			"name" : "select_lang",
			"children" :
			[
				{ "type" : "option",
					"value" : "fr",
					"label" : "Fran√ßais"
				},
				{ "type" : "option",
					"value" : "de",
					"label" : "Allemand"
				}
			]	
		},
		{	"type" : "text",
			"name" : "skipline",
			"value" : 1,
			"validators" : 
			[ 
				"validate-integer"
			]
		},
		{	"type" : "file",
			"name" : "dbfile",
			"validators" : 
			[ 
				"required"
			]
		},
		{	"type" : "submit",
			"name" : "submit",
			"class" : "btn btn-primary",
			"value" : "Submit"
		}
	]
}
